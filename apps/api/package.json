{
  "name": "api",
  "version": "0.0.0",
  "license": "MIT",
  "private": true,
  "scripts": {
    "dev": "nodemon --exec \"node -r esbuild-register ./src/index.ts\" -e .ts",
    "start": "node -r esbuild-register ./src/index.ts",
    "build": "tsc",
    "lint": "tsc --noEmit && TIMING=1 eslint src/**/*.ts* --fix",
    "test": "jest --detectOpenHandles",
    "validate": "npm-run-all -p lint test",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist"
  },
  "devDependencies": {
    "@types/body-parser": "^1.19.0",
    "@types/cors": "^2.8.10",
    "@types/express": "^4.17.12",
    "@types/jest": "^27.5.1",
    "@types/morgan": "^1.9.2",
    "@types/node": "^17.0.36",
    "@types/supertest": "^2.0.11",
    "config": "*",
    "esbuild": "^0.14.38",
    "esbuild-register": "^3.3.2",
    "eslint": "^8.15.0",
    "jest": "^28.1.0",
    "nodemon": "^2.0.15",
    "npm-run-all": "^4.1.5",
    "scripts": "*",
    "supertest": "^6.1.3",
    "tsconfig": "*",
    "typescript": "^4.5.3"
  },
  "jest": {
    "preset": "scripts/jest/node"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "logger": "*",
    "morgan": "^1.10.0"
  }
}
