{
  "name": "api",
  "version": "0.0.0",
  "license": "MIT",
  "private": true,
  "main": "src/index.ts",
  "scripts": {
    "dev": "cross-env NODE_ENV=development nodemon --exec \"node -r esbuild-register ./src/index.ts\" -e .ts",
    "start": "node -r esbuild-register ./src/index.ts",
    "build": "tsc",
    "prisma": "prisma",
    "lint": "tsc --noEmit && TIMING=1 eslint src/**/*.ts* --fix",
    "test": "jest --detectOpenHandles",
    "validate": "npm-run-all -p lint test",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist"
  },
  "prisma": {
    "seed": "node -r esbuild-register prisma/seed.ts"
  },
  "devDependencies": {
    "@types/body-parser": "^1.19.0",
    "@types/compression": "^1.7.2",
    "@types/cors": "^2.8.10",
    "@types/express": "^4.17.12",
    "@types/jest": "^27.5.1",
    "@types/morgan": "^1.9.2",
    "@types/node": "^17.0.36",
    "@types/supertest": "^2.0.11",
    "@types/swagger-jsdoc": "^6.0.1",
    "@types/swagger-ui-express": "^4.1.3",
    "cross-env": "^7.0.3",
    "esbuild": "^0.14.38",
    "esbuild-register": "^3.3.2",
    "eslint": "^8.15.0",
    "eslint-config-custom-api": "*",
    "jest": "^28.1.0",
    "nodemon": "^2.0.15",
    "npm-run-all": "^4.1.5",
    "prisma": "^3.14.0",
    "scripts": "*",
    "supertest": "^6.1.3",
    "tsconfig": "*",
    "typescript": "^4.5.3"
  },
  "jest": {
    "preset": "scripts/jest/node"
  },
  "dependencies": {
    "@prisma/client": "^3.14.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.0.1",
    "express": "^4.17.1",
    "helmet": "^5.1.0",
    "http-status": "^1.5.2",
    "joi": "^17.6.0",
    "morgan": "^1.10.0",
    "swagger-jsdoc": "^6.2.1",
    "swagger-ui-express": "^4.4.0",
    "winston": "^3.7.2",
    "xss-clean": "^0.1.1"
  }
}
